{
    "name": "strengthlogs",
    "version": "0.0.1",
    "description": "A Node-Based API for logging weight-training activities",
    "main": "app/app.js",
    "scripts": {
        "start": "pm2 start pm2.json",
        "stop": "pm2 stop pm2.json",
        "restart": "npm stop && npm start",
        "docs": "jsdoc -c jsdoc.json",
        "lint": "eslint app --ext .js",
        "preinstall": "npm install graceful-fs@4.2.4 && npx npm-force-resolutions",
        "dev": "nodemon app/app.js",
        "test:coverage": "nyc mocha test/*.js",
        "mocha": "nyc mocha --exit --recursive --timeout 30000",
        "test": "npm run lint && npm run mocha",
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "bunyan": "^1.8.15",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "graceful-fs": "^4.2.4",
        "helmet": "^4.6.0",
        "mysql": "^2.18.1",
        "pm2": "^5.1.1",
        "sqlstring": "^2.3.2",
        "validator": "^13.6.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "eslint": "^7.32.0",
        "jsdoc": "^3.6.7",
        "nodemon": "^2.0.12",
        "babel-eslint": "^10.1.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "chai-json": "^1.0.0",
        "husky": "^4.3.0",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "uuid": "^8.3.2"
    }
}
